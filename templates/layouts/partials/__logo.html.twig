{% if parametres.structure.filename %}
        <img src="{{ vich_uploader_asset(parametres.structure, 'imageFile') | imagine_filter('logo') }}" class="img-fluid" alt="{{ parametres.structure.name ?? company_name }}">
{% else %}
        <img src="{{ asset('assetics/images/logo/black-70@2x.png') }}" class="img-fluid" alt="{{ parametres.structure.name ?? company_name }}">
{% endif %}